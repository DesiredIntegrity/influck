    [1] = 1
}

game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("DailyRewardsService"):WaitForChild("RF"):WaitForChild("ClaimDailyReward"):InvokeServer(unpack(args))

